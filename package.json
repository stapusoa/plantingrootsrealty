{
  "name": "plantingrootsrealty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "packageManager": "pnpm@8.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "deploy:cms": "git subtree push --prefix=apps/cms cms main",
    "prepare": "is-ci || husky",
    "build": "pnpm turbo run build",
    "release": "semantic-release",
    "lint": "pnpm eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "pnpm eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "test": "vitest run",
    "check": "pnpm lint && pnpm test && pnpm build"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-storybook": "^9.1.1",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.9",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@sanity/image-url": "^1.2.0",
    "express": "^5.1.0"
  }
}
